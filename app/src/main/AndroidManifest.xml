<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.abdou.csemembersapp">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/iconnnn"
        android:label="@string/app_name"
        android:roundIcon="@drawable/iconnnn"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".splash_screen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".user_menu" />
        <activity
            android:name=".Add_event"
            android:parentActivityName=".user_menu"
            android:theme="@style/mytheme" />
        <activity android:name=".log_in"></activity>
    </application>

</manifest>